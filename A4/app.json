{
  "expo": {
    "name": "FitCast-Live",
    "slug": "FitCast-Live",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "exp",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/cb3235ed-972f-49d9-81d4-a52cd97d4150"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cb3235ed-972f-49d9-81d4-a52cd97d4150"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
